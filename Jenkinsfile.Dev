pipeline {
  environment {
    registry = "eran101/morse-server"
    registryCredential = 'dockerhub_id'
    dockerImage = 'latest'
  }
  agent any
    stages {
        stage('Git clone') {
            steps {
                git 'https://github.com/eran101/morse-server.git'
            }
        }
    }
}
