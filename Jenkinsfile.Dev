pipeline {
  environment {
    registry = "eran101/morse-server"
    registryCredential = 'dockerhub_id'
    dockerImage = 'latest'
  }
  agent any
    stages {
        stage('Git clone') {
            steps {
                git url: 'https://github.com/eran101/morse-server.git', 
                branch: 'main'
            }
        }
        stage('Build server image') {
          steps {
              docker.build("eran101/morse-server")
          }
        }
    }
}

